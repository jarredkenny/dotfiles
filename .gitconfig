[core]
	pager = diff-so-fancy | less --tabs=4 -RFX
	editor = nvim
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
[merge]
	tool = vscode
[mergetool "vscode"]
	tool = code --wait $MERGED
