#!/usr/bin/env bun

const natoAlphaMap = new Map([
  ["a", "alpha"],
  ["b", "bravo"],
  ["c", "charlie"],
  ["d", "delta"],
  ["e", "echo"],
  ["f", "foxtrot"],
  ["g", "golf"],
  ["h", "hotel"],
  ["i", "india"],
  ["j", "juliet"],
  ["k", "kilo"],
  ["l", "lima"],
  ["m", "mike"],
  ["n", "november"],
  ["o", "oscar"],
  ["p", "papa"],
  ["q", "quebec"],
  ["r", "romeo"],
  ["s", "sierra"],
  ["t", "tango"],
  ["u", "uniform"],
  ["v", "victor"],
  ["w", "whiskey"],
  ["x", "x-ray"],
  ["y", "yankee"],
  ["z", "zulu"],
]);

for await (const line of console) {
  const words = line.split(" ").filter(Boolean);
  for (const word of words) {
    for (const letter of word) {
      console.log(natoAlphaMap.get(letter) || letter);
    }
    console.log(" ");
  }
}
